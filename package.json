{
  "name": "sql-template-strings",
  "version": "2.2.2",
  "description": "ES6 tagged template strings for prepared statements with mysql and postgres",
  "main": "index.js",
  "typings": "index.d.ts",
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixfbecker/node-sql-template-strings"
  },
  "scripts": {
    "lint": "eslint index.js test",
    "test": "mocha \"test/**/*.js\"",
    "cover": "nyc --all mocha \"test/**/*.js\"",
    "typedoc": "typedoc --module es2015 --target es2015 --includeDeclarations --mode file --readme none --out typedoc index.d.ts"
  },
  "keywords": [
    "mysql",
    "mysql2",
    "postgres",
    "pg",
    "prepared",
    "statements",
    "placeholder",
    "es6",
    "tagged",
    "template",
    "strings"
  ],
  "author": "Felix Becker",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.12.0",
    "mocha": "^3.2.0",
    "mysql": "^2.12.0",
    "mysql2": "^1.1.2",
    "nyc": "^11.0.1",
    "pg": "^6.1.1",
    "sequelize": "^3.27.0",
    "typedoc": "^0.7.0"
  },
  "nyc": {
    "include": [
      "index.js"
    ],
    "exclude": [
      "test/**/*.js"
    ]
  }
}
